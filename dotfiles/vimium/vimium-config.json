# Insert your preferred key mappings here.

map s LinkHints.activateMode
map S LinkHints.activateModeToOpenInNewTab
map e Vomnibar.activateTabSelection

map l visitPreviousTab

map  <left> previousTab
map <right> nextTab
map <up> scrollUp
map <down> scrollDown

map <s-down> scrollPageDown
map <s-up> scrollPageUp

map <s-left> goBack
map <s-right> goForward
